<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 3圆角，白色背景绿边按钮 -->
    <!-- 圆角 -->
    <corners
        android:radius="5dp"/><!-- 设置圆角半径 -->

    <!-- 填充 -->
    <solid
        android:color="@android:color/white"/><!-- 填充的颜色 -->

    <!-- 描边 -->
    <stroke
        android:width="0.5dp"
        android:color="#CCCCCC"/>

</shape>
